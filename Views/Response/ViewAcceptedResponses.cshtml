@model IEnumerable<DeliveryBookingCllient.Models.RequestAccepted>

@{
    ViewData["Title"] = "Accepted Responses";
}
<partial name="_ExecutiveLoginPartial" />
<div class="row">
    <div class="col-md-4">

    </div>
    <div class="col-md-4">
        <h2 class="text-center text-warning">Accepted List</h2>

        <hr />
    </div>
    <div class="col-md-4">

    </div>
</div>
<hr />

@foreach (var item in Model)
{

    <h6>Executive: @ViewBag.ExecName</h6>
    <h6>Request-ID: @Html.DisplayFor(modelItem => item.RequestID)</h6>
    <h6>User's ID: @Html.DisplayFor(modelItem => item.UserID)</h6>
    <h6>Your Accept-ID: @Html.DisplayFor(modelItem => item.RejectID)</h6>
    <h6 class="text-success">Status Accepted!</h6>
    <br />
    @Html.ActionLink("Update Status", "UpdateAcceptedRequests", "Response", new { reqid = item.RequestID })
    <hr />
    
}

@ViewBag.errormsg
















