@model IEnumerable<DeliveryBookingCllient.Models.UserRequest>

@{
    ViewData["Title"] = "List of Requests";
    var data = ViewBag.Message;
}
<partial name="_ExecutiveLoginPartial" />
<div class="row">
    <div class="col-md-4">

    </div>
    <div class="col-md-4">
        <h2 class="text-center text-warning">Request List</h2>

        <hr />
    </div>
    <div class="col-md-4">

    </div>
</div>
<hr />

@foreach (var item in Model)
{
    <h4 class="text-primary">Executive: @ViewBag.ExecName</h4>
    <h6>RequestID : @Html.DisplayFor(modelItem => item.RequestID)</h6>
    <h6>Date of Pickup: @Html.DisplayFor(modelItem => item.DTofPickup)</h6>
    <h6>Weight of Package: @Html.DisplayFor(modelItem => item.WeightOfPackage)</h6>
    <h6>Address: @Html.DisplayFor(modelItem => item.Address)</h6>

    <br />
    <p>
        @Html.ActionLink("Select", "Create", "Response",
       new { reqid = item.RequestID, userid = item.UserID })
</p>
<hr />
}

<br />
@Html.ActionLink("Update Status of Accepted Requests", "ViewAcceptedResponses")









