@model DeliveryBookingCllient.Models.ExecutiveResponse

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ExecutiveResponse</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="status"/> @Html.DisplayNameFor(model => model.status)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="Price" class="control-label"></label>
                <input asp-for="Price" class="form-control" />
                <span asp-validation-for="Price" class="text-danger"></span>
            </div>
            <div class="form-group">
                @*<label asp-for="RequestID" class="control-label"></label>*@
                <input hidden asp-for="RequestID" class="form-control" value="@ViewBag.ReqID"/>
                @*<span asp-validation-for="RequestID" class="text-danger"></span>*@
            </div>
            <div class="form-group">
                <input type="submit" value="Ok" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
</div>

@ViewBag.errormsg

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
