@model IEnumerable<DeliveryBookingCllient.Models.City>

@{
    ViewData["Title"] = "Cities";
}

<h1>Cities</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CityID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CityName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CityID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CityName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.CityID }) |
                    @*@Html.ActionLink("Details", "Details", new { id = item.CityID }) |*@
                    @Html.ActionLink("Delete", "Delete", new { id = item.CityID })
                </td>
            </tr>
        }
        
    </tbody>
</table>

@ViewBag.errormsg